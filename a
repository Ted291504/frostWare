local function RPHWV_fake_script() -- Execute.LocalScript 
	local script = Instance.new('LocalScript', Execute)

	local TextBox = script.Parent.Parent
	
	local ExecuteButton = script.Parent
	ExecuteButton.MouseButton1Click:Connect(function()
		local luaCode = TextBox.Text
		local success, err = pcall(function()
			loadstring(luaCode)()
		end)
	
		if not success then
			warn("Your gay you gay boy your so fat the code wouldnt run" .. err)
		end
	end)
	
end
coroutine.wrap(RPHWV_fake_script)()
