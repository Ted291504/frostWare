	-- a
end
coroutine.wrap(MATKWF_fake_script)()
local function PPLP_fake_script() -- Search.LocalScript 
	local script = Instance.new('LocalScript', Search)

	local HttpService = game:GetService("HttpService")
	
	local searchBox = script.Parent
	local searchButton = searchBox:FindFirstChild("TextButton")
	local resultsFrame = searchBox:FindFirstChild("ScrollingFrame")
	
	-- Function to create script display buttons
	local function createScriptButton(script, index)
		local button = Instance.new("TextButton")
		local UICorner = Instance.new("UICorner")
		local UIListLayout = resultsFrame:FindFirstChild("UIListLayout") or Instance.new("UIListLayout")
	
		button.Parent = resultsFrame
		button.BackgroundColor3 = Color3.fromRGB(81, 81, 81)
		button.BorderSizePixel = 0
		button.Position = UDim2.new(0, 0, 0, (index - 1) * 45)
		button.Size = UDim2.new(1, -10, 0, 40)  -- Adjust size and padding
		button.TextColor3 = Color3.fromRGB(255, 255, 255)
		button.TextSize = 14
		button.Text = script.title or "No title available"
		button.TextScaled = true
		button.TextWrapped = true
	
		UICorner.Parent = button
	
		-- Ensure UIListLayout is added for proper spacing
		if not resultsFrame:FindFirstChild("UIListLayout") then
			UIListLayout.Parent = resultsFrame
			UIListLayout.FillDirection = Enum.FillDirection.Vertical
			UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
			UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
			UIListLayout.Padding = UDim.new(0, 5)
		end
	
		-- Execute script on button click
		button.MouseButton1Click:Connect(function()
			local scriptContent = script.script or ""
			if scriptContent ~= "" then
				local success, err = pcall(function()
					loadstring(scriptContent)()
				end)
				if not success then
					warn("Failed to execute script: " .. err)
				end
			end
		end)
	end
	
	-- Function to fetch scripts from the API
	local function fetchScripts(query)
		local url = "https://scriptblox.com/api/script/search?filters=free&q=" .. query
		local success, response = pcall(function()
			return game:HttpGet(url)
		end)
	
		if not success then
			warn("Failed to fetch scripts: " .. response)
			return {}
		end
	
		local success, data = pcall(function()
			return HttpService:JSONDecode(response)
		end)
	
		if not success then
			warn("Failed to decode JSON: " .. data)
			return {}
		end
	
		return data.result.scripts or {}
	end
	
	-- Function to clear previous search results
	local function clearResults()
		for _, child in ipairs(resultsFrame:GetChildren()) do
			if child:IsA("TextButton") then
				child:Destroy()
			end
		end
	end
	
	-- Search button click event
	searchButton.MouseButton1Click:Connect(function()
		local query = searchBox.Text
		if query ~= "" then
			clearResults()
			local scripts = fetchScripts(query)
			for i, script in ipairs(scripts) do
				createScriptButton(script, i)
			end
		end
	end)
	
end
coroutine.wrap(PPLP_fake_script)()
